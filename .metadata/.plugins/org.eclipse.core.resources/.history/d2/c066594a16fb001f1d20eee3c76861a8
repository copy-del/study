package 배열실습;

import java.util.Scanner;

public class Ex03배열실습_예제_3의배수 {

	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);

		int[] x = new int[10];
		int sum = 0;
		for (int i = 0; i < x.length; i++) {
			System.out.print(i + "번 째 정수 입력 : ");
			x[i] = sc.nextInt();

		}
		System.out.print("3의 배수 : ");
		int b;
		for (int i = 0; i < x.length; i++) {
			if (x[i] % 3 == 0) {
				b = x[i];
				System.out.print(b + " ");
			}
		}

	}

}
