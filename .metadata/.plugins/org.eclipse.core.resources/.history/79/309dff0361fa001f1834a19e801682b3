package 이중for문;

import java.util.Scanner;

public class Ex09로또번호 {

	public static void main(String[] args) {
//		(로또 번호 뽑기) (프로그래머스 문제는 아니에요!)★★
//		랜덤수 추출을 활용해서, 로또 번호 6개 뽑기!
//		(로또번호는 1~45 범위예요! 중복 번호가 뜰 수 있는데... 그거 해결하면 별 4개짜리 문제....)
		
		Scanner random = null;
		@SuppressWarnings("null")
		int rannum1 = random.nextInt(45);
		int rannum2 = random.nextInt(45);
		int rannum3 = random.nextInt(45);
		int rannum4 = random.nextInt(45);
		int rannum5 = random.nextInt(45);
		int rannum6 = random.nextInt(45);
		System.out.println(rannum1+rannum2+rannum3+rannum4+rannum5+rannum6);
		
		

	}

}
